%img{'ng-src' => '{{restaurant.logoUrl}}'}
.details
  %span.name {{restaurant.name}}
  %span.category{'ng-show' => 'restaurant.category'} ⦿ {{restaurant.category}}
  .tags
    .tag{'ng-repeat' => 'tag in restaurant.tags'}
      %span.name {{tag.name}}
      %span.multiplier ×
      %span.quantity {{tag.quantity}}
  .voters
    .voter{'ng-repeat' => 'voter in restaurant.getVoters()'}
      %img.avatar{'ng-src' => '{{voter.avatarUrl}}'}
    .btn.vote-btn I want in!